{
  "name": "gulp-extract-css-urls",
  "version": "0.3.0",
  "description": "Imports to the pipeline all the CSS assets included with url()",
  "main": "lib/index.js",
  "keywords": [
    "gulpplugin",
    "gulp",
    "less",
    "watch",
    "sourcemap"
  ],
  "repository": {
    "type": "git",
